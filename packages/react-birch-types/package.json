{
  "name": "react-birch-types",
  "version": "2.0.0",
  "description": "Common type definitions for react-birch",
  "main": "dist/index.js",
  "browser:module": "src/index.ts",
  "types": "src/index.ts",
  "author": "Tinia Labs <admin@tinialabs.org>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinialabs/birch-tree.git"
  },
  "keywords": [
    "react",
    "virtualized",
    "file",
    "monaco",
    "vscode",
    "tree",
    "nested",
    "object",
    "list"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "prepack": "npm run lint && npm run build && npm run generate-docs",
    "postpublish": "npm run publish-docs",
    "generate-docs": "typedoc --mode file --excludePrivate --excludeProtected --excludeNotExported --theme node_modules/typedoc-clarity-theme/bin --out dist/docs ./src",
    "publish-docs": "gh-pages -d dist/docs"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.20.36",
    "typedoc-clarity-theme": "^1.1.0"
  }
}