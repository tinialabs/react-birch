{
  "name": "react-birch-headless",
  "version": "2.0.0",
  "description": "Stylish solution for rendering nested trees in React apps",
  "main": "src/index.ts",
  "browser:module": "src/index.ts",
  "types": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinialabs/react-birch.git"
  },
  "scripts": {
    "test": "echo TESTS TODO!!!",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint --project .",
    "prepack": "npm run lint && npm run build && npm run test && npm run generate-docs",
    "postpublish": "npm run publish-docs",
    "generate-docs": "typedoc --mode file --excludePrivate --excludeProtected --excludeNotExported --theme node_modules/typedoc-clarity-theme/bin --out dist/docs ./src",
    "publish-docs": "gh-pages -d dist/docs"
  },
  "keywords": [
    "react",
    "virtualized",
    "file",
    "monaco",
    "vscode",
    "tree",
    "nested",
    "object",
    "list"
  ],
  "devDependencies": {
    "@types/p-series": "^1.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-window": "^1.8.3",
    "gh-pages": "^3.1.0",
    "typedoc": "^0.20.36",
    "typedoc-clarity-theme": "^1.1.0",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6"
  },
  "dependencies": {
    "react-birch-event-emitter": "^2.0.0",
    "p-series": "^2.1.0",
    "path-fx": "^2.1.1",
    "react-window": "^1.8.6",
    "mobx": "^4.15.4"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "author": "Tinia Labs <admin@tinialabs.org>",
  "license": "MIT"
}